{
  "name": "gfin",
  "version": "0.0.1",
  "scripts": {
    "dev": "bun run --hot --silent --no-clear-screen ./backend/src/index.tsx --dev",
    "prod": "bun run --silent --no-clear-screen ./backend/src/index.tsx",
