    "react-hook-form": "^7.56.1",
    "react-phone-number-input": "^3.4.12",
    "rlib": "https://github.com/rizrmd/rlib.git",
    "valtio": "^2.1.4"
  },
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ]
